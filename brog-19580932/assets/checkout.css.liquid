/** 
 * Black & White
 * Checkout CSS
 */
 
{% assign font_heading_type_split = settings.font_heading_type | split: '|' %}
{% if font_heading_type_split.first == 'GWF' %}
    {% if font_heading_type_split.last == 'CUSTOM' %}
        {% capture font_heading_type %}'{{ settings.font_heading_type_custom | split: ':' | first | replace:'+',' ' }}'{% endcapture %}
    {% else %}
        {% capture font_heading_type %}'{{ font_heading_type_split.last | split: ':' | first | replace:'+',' ' }}'{% endcapture %}
    {% endif %}
{% else %}
    {% assign font_heading_type = settings.font_heading_type %}
{% endif %}

{% assign font_logo_type_split = settings.font_logo_type | split: '|' %}
{% if font_logo_type_split.first == 'GWF' %}
    {% if font_logo_type_split.last == 'CUSTOM' %}
        {% capture font_logo_type %}'{{ settings.font_logo_type_custom | split: ':' | first | replace:'+',' ' }}'{% endcapture %}
    {% else %}
        {% capture font_logo_type %}'{{ font_logo_type_split.last | split: ':' | first | replace:'+',' ' }}'{% endcapture %}
    {% endif %}
{% else %}
    {% assign font_logo_type = settings.font_logo_type %}
{% endif %}

{% assign font_standard_type_split = settings.font_standard_type | split: '|' %}
{% if font_standard_type_split.first == 'GWF' %}
    {% if font_standard_type_split.last == 'CUSTOM' %}
        {% capture font_standard_type %}'{{ settings.font_standard_type_custom | split: ':' | first | replace:'+',' ' }}'{% endcapture %}
    {% else %}
        {% capture font_standard_type %}'{{ font_standard_type_split.last | split: ':' | first | replace:'+',' ' }}'{% endcapture %}
    {% endif %}
{% else %}
    {% assign font_standard_type = settings.font_standard_type %}
{% endif %}

{% if settings.font_heading_type == 'GWF|CUSTOM' %}
    @import url(//fonts.googleapis.com/css?family={{ settings.font_heading_type_custom }}:100,300,400,700,300italic,400italic,700italic);
{% elsif font_heading_type_split.first == 'GWF' %}
    @import url(//fonts.googleapis.com/css?family={{ font_heading_type_split.last }});
{% endif %}
{% if settings.font_logo_type == 'GWF|CUSTOM' %}
    @import url(//fonts.googleapis.com/css?family={{ settings.font_logo_type_custom }}:100,300,400,700,300italic,400italic,700italic);
{% elsif font_logo_type_split.first == 'GWF' %}
    @import url(//fonts.googleapis.com/css?family={{ font_logo_type_split.last }});
{% endif %}
{% if settings.font_standard_type == 'GWF|CUSTOM' %}
    @import url(//fonts.googleapis.com/css?family={{ settings.font_standard_type_custom }}:100,300,400,700,300italic,400italic,700italic);
{% elsif font_standard_type_split.first == 'GWF' %}
    @import url(//fonts.googleapis.com/css?family={{ font_standard_type_split.last }}:100,300,400,700,300italic,400italic,700italic);
{% endif %}

body {
    background:{{ settings.color_bg_main }}
    {% if settings.bg_image_body && settings.bg_image_body != '' %}
    url({{ settings.bg_image_body | asset_url }}) {{ settings.bg_repeat }} center {{ settings.bg_offset_y }}
    {% endif %};
    font:{{ settings.font_standard_style }} {{ settings.font_standard_size }}px {{ font_standard_type }}, sans-serif;
    line-height: 1.4em;
    color:{{ settings.color_body_standard }};
}

h1, h2, h3 {
    color:{{ settings.color_body_standard }};
}

a {
    color:{{ settings.color_body_standard }};
    text-decoration:underline;
}

#info-bar {
    padding:10px 0;
    background:{{ settings.color_bg_header }};
    color:{{ settings.color_head_general }};
}

#header #tagline .title {
    color:{{ settings.color_body_shoptitle }};
    font:{{ settings.font_logo_style }} {{ settings.font_logo_size }}px {{ font_logo_type }}, sans-serif;
}

{% if settings.use_logo_image %}
#header #logo {
    display:block;
    height:{{ settings.logo_height }}px;
    background:url({{ 'logo.png' | asset_url }}) no-repeat
    {% if settings.logo_margin_left == 'center' or settings.logo_margin_left == 'centre' %}
    center
    {% else %}
    {{ settings.logo_margin_left }}px
    {% endif %} 0;
    margin-top:{{ settings.logo_margin_top }}px;
}
#header #tagline .title {
    display:none;
}
{% endif %}

#header #tagline .subtitle {
    display:block;
}

#header,
#main,
#footer,
#container.slim #main,
#container.slim #header,
#container.slim #footer {
    background:none;
}

#container {
    font-size:100%;
}
#container.slim {
  background: none !important;
}

input[type=submit] {
    border:0;
    background:{{ settings.color_btn_bg }};
    color: {{ settings.color_btn_text }};
    padding:7px 15px;
    cursor:pointer;
    display:inline-block;
    zoom:1;
    *display:inline;
    
    -webkit-box-shadow: 0 0 2px 0 #000;
    -moz-box-shadow: 0 0 2px 0 #000;
    box-shadow: 0 0 2px 0 #000;

    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
}
input {
    font:{{ settings.font_standard_style }} {{ settings.font_standard_size }}px {{ font_standard_type }}, sans-serif;
}
#buttons input {
    font-weight:normal;
    padding:7px 15px;
}

#email label,
.group h3,
.pgroup h3,
#cost,
#order-num,
#steps,
#header #tagline .subtitle {
    font: {{ settings.font_standard_style }} 24px {{ font_heading_type }}, sans-serif;
    line-height:30px;
    border-bottom:1px solid {{ settings.color_general_divider }};
    margin-bottom: 20px;
    padding-top:20px;
    padding-bottom:10px;
    color:#333;
}

#cost, #order-num {
    font-size:220%;
    border:none;
    text-shadow:none;
    padding-top:35px;
    padding-bottom:0px;

}

#steps {
}

#thumbs div,
#products {
    border-color:{{ settings.color_general_divider }};
    font-size: 120%;
}

#overview {
    background:none;
    border:none;
}

#buttons {
    background:#f7f7f7;
    border:1px solid #ccc;
  text-align: right;
}

h4.active-customer,
#header #tagline .subtitle,
#overview h2,
#footer,
#thumbs h3,
#thumbs span,
#overview .hint,
#thumbs h3 span,
#cost,
#steps {
    color:{{ settings.color_body_standard }};
}

#wallet {
    background-image:none !important;
    background-color:transparent !important;
}

label {
    font-weight:300;
}

.form td {
    vertical-align:middle;
}

.form td input {
    padding:4px;
}

/* Enable the following styles if the sides of your logo
 * get cut off in the checkout due to it being very wide
 */

/*
#container.slim,
#container.slim #header,
#container.slim #main,
#container.slim #footer{
    width:776px;
}
*/

/* PurpleTag updates / Lisa */
li#billing_address_province_li, li#shipping_address_province_li {
    display: none;
}
div#header #tagline {
    display: none;
}
/* Required Phone Number */
label[for=billing_address_phone]:after, 
label[for=shipping_address_phone]:after { 
  content: '*'; 
  padding-left: 3px; 
  display: inline; 
  color: red; 
}

/* Hide the delivery method selection, as we only offer one delivery method /Fred */
div#payment p.sst {
  display: none;
}

/* add a little space to the button, so it does not look cramped with the text /Fred */
input#complete-purchase, input#commit-button {
  margin-right: 0.5em;
  font-size: 24px;
  box-shadow: 0 0 2px 1px #ddd;
}
input#complete-purchase:hover, input#commit-button:hover {
  box-shadow: 0 0 2px 1px #000;
}

/* use a little larger text in the input, to make it easer to use /Fred */
form input, form select {
  font-size: 100%;
}

/* use green instead of red, feels more friendly /Fred */
div#shipping-same {
  background-color: #EEFFEB;
  border-color: #B9E7BD;
  text-align: left;
}

div#shipping-same span {
  opacity: 0.5;
}

span#steps {
  display: none;
}

#email label, .group h3 {
  padding-top: 0;
  margin-bottom: 10px;
  padding-bottom: 0;
  border-bottom: 0;
}

#info-bar {
  background-color: #eee;
}

#container {
  background-position: 100% 10px !important;
}

/* Hide more information /Fred */
#discounts h3 {
  display: none;
}
#wallet {
  display: none;
}
#products {
  border-right: 0 !important;
}
#products h2 {
  display: none;
}
#email label {
  font-size: 18px;
  margin-bottom: 5px;
  line-height: normal;
}
#email p {
  margin: 0 0 5px 0;
}
table#billing tbody tr:nth-child(3),
table#shipping tbody tr:nth-child(3) {
  display: none;
}
#footer.footer {
  border-top: 1px solid #ddd;
  padding-top: 5px;
}
div#buttons, table#buttons {
  border: 0;
  background: none;
}
#marketing-box h3 {
  display: none;
}
#discounts div.group {
  margin: 0;
  background: none;
  border: 0;
  opacity: 0.7;
  padding: 0;
  text-align: right;
}
#payment div.group {
  background: none;
  border: 0;
  margin: 0;
  padding: 0;
}
#payment div.group h3 {
  margin: 0;
  padding: 0;
}
#payment div.group p {
  display: none;
}
#marketing-box {
  background: none;
  border: 0;
  margin: 0;
  padding: 0; 
}
small#cancel-purchase {
  display: none;
}


@font-face {
    font-family: "mobileicons";
    src: url(data:application/font-woff;charset=utf-8;base64,) format("woff"), 
    url("{{ 'mobileicons.ttf' | asset_url }}") format("truetype");
    font-weight: normal;
    font-style: normal;
}
.sharing {
  text-align: left;
  font-family: mobileicons;
  margin-top: 15px;
}
#content .sharing a.share {
  display: inline-block;
  width: 21%;
  margin: 0;
  color: white;
  padding: 20px 0 5px 2px;
  font-size: 24px;
  text-align: left;
  vertical-align: baseline;
  border: 0;
  text-decoration: none;
}
.sharing .share span {
  display: none;
}
.share.facebook::before {
  content: "\e012";
}
.share.facebook {
  background-color: rgb(60, 88, 152);
}
.share.twitter::before {
  content: "\e00f"; 
}
.share.twitter {
  background-color: rgb(106, 173, 209);
}
.share.pin::before {
  content: "\e010"; 
}
.share.pin {
  background-color: #de2108;
}
.share.gplus::before {
  content: "\e604"; 
}
.share.gplus {
  background-color: rgb(205, 54, 39);
}
.share.whatsapp::before {
  content: "\e014";
}
.share.whatsapp {
  background-color: #58AD15;
}
#overview #thumbs {
  display: none;
}
#overview h2 {
  font-size: 50px;
  margin-top: 1em;
  text-align: center;
}
body .group,
body #addresses,
body #email {
  border-radius: 0;
  border: none;
  border-top: 1px solid #e6e6e6;
  background: none;
}
body #email {
  border: 0;
}
body {
  margin: 0;
  padding: 0 10px;
}
body #container {
  margin: 0 auto;
}