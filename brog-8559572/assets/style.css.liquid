#overlay {
    display: none; /* ensures its invisible until its called */
    position: fixed; /* makes the div go into a position thats absolute to the browser viewing area */
    left: 0%; /* positions the div half way horizontally */
    top: 25%; /* positions the div half way vertically */
    width: 100%;
    color: #666;
    text-align: center;
    font-size: 18px;
    z-index: 100; /* makes the div the top layer, so itll lay on top of the other content */
  background-image:url('{{ 'loader-white.gif' | asset_url }}');
    background-repeat: no-repeat;
    background-position: 50% 25px;
    padding-top: 60px;
}
#fade {
    display: none;  /* ensures its invisible until its called */
    position: fixed;  /* makes the div go into a position thats absolute to the browser viewing area */
    left: 0%; /* makes the div span all the way across the viewing area */
    top: 0%; /* makes the div span all the way across the viewing area */
    background-color: #eee;
    -moz-opacity: 0.9; /* makes the div transparent, so you have a cool overlay effect */
    opacity: .90;
    filter: alpha(opacity=90);
    width: 100%;
    height: 100%;
    z-index: 90; /* makes the div the second most top layer, so itll lay on top of everything else EXCEPT for divs with a higher z-index (meaning the #overlay ruleset) */
}
